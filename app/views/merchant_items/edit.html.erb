<%= form_with model: [@merchant, @item], local: true, html: { id: 'Update_Item' } do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :description %>
  <%= form.text_field :description %>

  <%= form.label :unit_price %>
  <%= form.text_field :unit_price %>

  <%= form.submit %>
<% end %>
