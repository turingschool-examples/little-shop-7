<%= form_with url: merchant_items_path(@merchant), method: :post, data: {turbo: false} do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name%>

  <%= form.label :description %>
  <%= form.text_field :description%>

  <%= form.label :unit_price %>
  <%= form.number_field :unit_price %>


  <%= form.submit 'Submit' %>
<% end %>